import $core;
do {
  $core:print_ln("a");
  def $p0 := new $core:Promise();
  def $p1 := $p0.then(fn => $core:print_ln(1));
  def $p2 := $p1.then(fn => $core:print_ln(2));
  def $p3 := $p2.then(fn {
    $core:print_ln(3);
    $core:print_ln($p0.get());
    $core:print_ln($p1.get());
    $core:print_ln($p2.get());
  });
  $core:print_ln("b");
  $core:delay(fn {
    $core:print_ln("c");
    $p0.fulfill!(0);
    $core:print_ln("d");    
  });
  $core:print_ln("e");
}
