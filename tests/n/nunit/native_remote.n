import $assert;
import $core;

def $test_time_call() {
  def $time_p := @core:time->call();
  $assert:not($time_p.is_resolved?);
  $time_p.then(fn ($value) {
    $assert:equals(52, $value);
  });
}

do {
  $test_time_call();
}
