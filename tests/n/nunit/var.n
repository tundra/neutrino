import $assert;

def $test_var() {
  var $x := 3;
  $assert:equals(3, $x);
  $x := 4;
  $assert:equals(4, $x);
  $x := 3;
  $assert:equals(3, $x);
}

def $main() {
  $test_var();
}

entry_point $main();
