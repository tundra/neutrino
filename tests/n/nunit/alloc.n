import $assert;
import $collection;
import $core;

# Loop around allocating memory to cause a garbage collection.
def $test_blank_allocation() {
  for $i in (0 .to 1000) do {
    new @core:Tuple(20 * ($i + 1));
  }
}

do {
  $test_blank_allocation();
}
